{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000/",
  "resourcePath": "changes",
  "apis": [
    {
      "path": "api/v1/changes.json",
      "operations": [
        {
          "summary": "Fetches all Changes",
          "parameters": [
            {
              "paramType": "query",
              "name": "page",
              "type": "integer",
              "description": "Page number",
              "required": false
            },
            {
              "paramType": "query",
              "name": "order",
              "type": "string",
              "description": "Sort order",
              "required": false,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "created_at DESC",
                  "created_at ASC",
                  "updated_at DESC",
                  "updated_at ASC"
                ]
              }
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "message": "Unauthorized"
            },
            {
              "code": 406,
              "message": "The request you made is not acceptable"
            },
            {
              "code": 416,
              "message": "Requested Range Not Satisfiable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::Changes#index"
        }
      ]
    },
    {
      "path": "api/v1/changes/{id}.json",
      "operations": [
        {
          "summary": "Fetches a single Change",
          "parameters": [
            {
              "paramType": "path",
              "name": "id",
              "type": "string",
              "description": "Change ID",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "message": "Unauthorized"
            },
            {
              "code": 404,
              "message": "Not Found"
            },
            {
              "code": 406,
              "message": "Not Acceptable"
            }
          ],
          "method": "get",
          "nickname": "Api::V1::Changes#show"
        }
      ]
    }
  ]
}